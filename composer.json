{
    "name": "clausmunch/filament-changelog",
    "description": "Displays a changelog based on GitHub releases in Filament.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Claus Munch",
            "email": "claus@codepoint.dk"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0 || ^11.0",
        "illuminate/http": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/view": "^10.0 || ^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "ClausMunch\\FilamentChangelog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClausMunch\\FilamentChangelog\\ChangelogPluginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}